{
  "name": "sold-services-placester",
  "version": "1.0.0",
  "description": "Domain Driven Design",
  "main": "./src/server.ts",
  "scripts": {
    "migrate": "tsc config/database.ts && npm run start:migration",
    "start:migration": "npx sequelize db:migrate",
    "swagger-gen": "node src/swagger.js",
    "local": "cross-env NODE_PATH=. NODE_ENV=local nodemon --watch './**/*.ts' --exec 'ts-node' src/server.ts",
    "pm2-non-local": "cross-env NODE_PATH=. pm2 start ./src/server.js --watch -i max",
    "build:local": "cross-env NODE_PATH=. NODE_ENV=local ./pipeline/scripts/build.sh",
    "build:internal": "cross-env NODE_PATH=. NODE_ENV=internal ./pipeline/scripts/build.sh",
    "build:staging": "cross-env NODE_PATH=. NODE_ENV=staging ./pipeline/scripts/build.sh",
    "build:production": "cross-env NODE_PATH=. NODE_ENV=production ./pipeline/scripts/build.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.15",
    "awilix": "^2.12.0",
    "axios": "^0.25.0",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dd-trace": "^2.10.0",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "express-routes-catalogue": "^1.1.0",
    "http-status": "^1.5.0",
    "mongoose": "^6.2.0",
    "mysql2": "^2.3.3",
    "pm2": "^5.2.0",
    "sequelize": "^6.20.1",
    "structure": "^2.0.1",
    "swagger-autogen": "^2.21.5",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1"
  }
}
